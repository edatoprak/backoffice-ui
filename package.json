{
    "name": "backoffice",
    "version": "0.0.1",
    "description": "Backoffice",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "node --max_old_space_size=8096 ./node_modules/@angular/cli/bin/ng serve --port 4200",
        "start-host": "node --max_old_space_size=4048 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0",
        "start-ssl": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --ssl true --ssl-key /ssl/server.key --ssl-cert /ssl/server.crt ",
        "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
        "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
        "build": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --prod --outputHashing=all --vendor-chunk --common-chunk --base-href /",
        "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
        "build-prod": "node --max_old_space_size=10240 ./node_modules/@angular/cli/bin/ng build --prod --outputHashing=all --vendor-chunk --common-chunk --base-href /",
        "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json"
    },
    "dependencies": {
        "@angular/animations": "12.2.1",
        "@angular/cdk": "12.2.1",
        "@angular/common": "12.2.1",
        "@angular/compiler": "12.2.1",
        "@angular/core": "12.2.1",
        "@angular/forms": "12.2.1",
        "@angular/material": "12.2.1",
        "@angular/material-moment-adapter": "12.2.1",
        "@angular/platform-browser": "12.2.1",
        "@angular/platform-browser-dynamic": "12.2.1",
        "@angular/router": "12.2.1",
        "@fullcalendar/angular": "4.4.5-beta",
        "@fullcalendar/core": "4.4.2",
        "@fullcalendar/daygrid": "4.4.2",
        "@fullcalendar/interaction": "4.4.2",
        "@fullcalendar/list": "4.4.2",
        "@fullcalendar/moment": "4.4.2",
        "@fullcalendar/rrule": "4.4.2",
        "@fullcalendar/timegrid": "4.4.2",
        "@ngneat/transloco": "2.22.0",
        "@ngxs/router-plugin": "^3.7.2",
        "@ngxs/store": "^3.7.2",
        "apexcharts": "3.27.3",
        "bignumber.js": "^9.0.1",
        "crypto-js": "3.3.0",
        "highlight.js": "11.2.0",
        "jasmine": "^3.9.0",
